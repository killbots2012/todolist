odoList todos={todos} toggleTodo={toggleTodo} />\n    <input ref={todoNameRef} type=\"text\" />\n    <button onClick={handleAddTodo}>Add Todo</button>\n    <button onClick={handleClearTodos}>Clear Complete</button>\n    <div>{todos.filter(todo => !todo.complete).length} left to do</div>\n  </>\n)\nexport default App;\n","/home/frank/reactProjects/todoList/src/TodoList.js",[],{"ruleId":"35","severity":1,"message":"36","line":8,"column":10,"nodeType":"37","messageId":"38","endLine":8,"endColumn":15},{"ruleId":"35","severity":1,"message":"39","line":8,"column":17,"nodeType":"37","messageId":"38","endLine":8,"endColumn":25},{"ruleId":"35","severity":1,"message":"40","line":9,"column":9,"nodeType":"37","messageId":"38","endLine":9,"endColumn":20},{"ruleId":"41","severity":2,"message":"42","line":13,"column":1,"nodeType":"37","endLine":13,"endColumn":10},{"ruleId":"43","severity":2,"message":"44","line":14,"column":34,"nodeType":"37","messageId":"45","endLine":14,"endColumn":46},{"ruleId":"43","severity":2,"message":"46","line":15,"column":20,"nodeType":"37","messageId":"45","endLine":15,"endColumn":28},{"ruleId":"47","severity":2,"message":"48","line":17,"column":5,"nodeType":"49","messageId":"50","endLine":17,"endColumn":12},{"ruleId":"43","severity":2,"message":"51","line":17,"column":5,"nodeType":"37","messageId":"45","endLine":17,"endColumn":12},{"ruleId":"41","severity":2,"message":"42","line":21,"column":1,"nodeType":"37","endLine":21,"endColumn":10},{"ruleId":"43","severity":2,"message":"52","line":22,"column":58,"nodeType":"37","messageId":"45","endLine":22,"endColumn":63},{"ruleId":"43","severity":2,"message":"52","line":23,"column":5,"nodeType":"37","messageId":"45","endLine":23,"endColumn":10},{"ruleId":"43","severity":2,"message":"52","line":26,"column":24,"nodeType":"37","messageId":"45","endLine":26,"endColumn":29},{"ruleId":"43","severity":2,"message":"53","line":32,"column":16,"nodeType":"37","messageId":"45","endLine":32,"endColumn":27},{"ruleId":"43","s